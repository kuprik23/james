{
  "name": "cybercat-scanner",
  "version": "2.0.0",
  "description": "CyberCAT Scanner - Military-Grade Vulnerability Scanner (TypeScript Edition)",
  "main": "dist/scanner.js",
  "bin": {
    "cybercat-scanner": "./dist/scanner.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/scanner.js",
    "watch": "tsc --watch",
