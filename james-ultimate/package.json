{
  "name": "james-ultimate",
  "version": "2.0.0",
  "description": "James Ultimate - AI-Powered Cybersecurity Platform with Multi-LLM Support (TypeScript + Java)",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "bin": {
    "james": "./dist/main.js"
  },
  "scripts": {
    "start": "node dist/main.js",
    "server": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/main.ts",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "build": "npm run build:ts && npm run build:java && npm run build:kotlin && npm run build:rust && npm run build:cpp",
    "build:ts": "tsc",
    "build:java": "cd java-scanner && mvn clean package",
    "build:kotlin": "cd kotlin-scanner && gradle build",
    "build:rust": "cd rust-crypto && cargo build --release",
    "build:cpp": "cd cpp-scanner && cmake -B build -S . && cmake --build build --config Release",
    "build:exe": "node build-exe.js",
    "build:exe:legacy": "pkg dist/main.js --targets node18-win-x64 --output dist/James.exe",
    "build-all": "npm run build && node build-exe.js --all",
    "clean": "rimraf dist && cd java-scanner && mvn clean && cd ../kotlin-scanner && gradle clean && cd ../rust-crypto && cargo clean && cd ../cpp-scanner && rimraf build",
    "watch": "tsc --watch",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "setup": "node setup-prerequisites.bat",
    "check-deps": "node -e \"console.log('Checking dependencies...'); const deps = ['rustc', 'cargo', 'mvn', 'gradle', 'cmake', 'java', 'javac', 'kotlinc']; deps.forEach(d => { try { require('child_process').execSync(d + ' --version', {stdio: 'ignore'}); console.log('✓ ' + d); } catch(e) { console.log('✗ ' + d + ' (missing)'); } });\"",
    "check-prerequisites": "node check-prerequisites.js",
    "check": "node check-prerequisites.js",
    "auto-install": "powershell -ExecutionPolicy Bypass -File auto-install-prerequisites.ps1"
  },
  "pkg": {
    "assets": [
      "src/**/*",
      "public/**/*",
      "config/**/*"
    ],
    "outputPath": "dist"
  },
  "keywords": [
    "cybersecurity",
    "ai",
    "llm",
    "security",
    "iot",
    "ollama",
    "openai",
    "anthropic"
  ],
  "author": "CYBERCAT Security Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "boxen": "^5.1.2",
    "cli-table3": "^0.6.3",
    "axios": "^1.6.2",
    "node-fetch": "^2.7.0",
    "systeminformation": "^5.21.20",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "marked": "^11.0.0",
    "highlight.js": "^11.9.0",
    "open": "^8.4.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "sqlite3": "^5.1.6",
    "stripe": "^14.9.0"
  },
  "optionalDependencies": {
    "java": "^0.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/inquirer": "^9.0.7",
    "@types/chalk": "^2.2.0",
    "@types/ws": "^8.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.2",
    "eslint": "^8.54.0",
    "pkg": "^5.8.1",
    "@vercel/ncc": "^0.38.1",
    "nodemon": "^3.0.2",
    "nexe": "^4.0.0-rc.6",
    "rimraf": "^5.0.5"
  }
}
