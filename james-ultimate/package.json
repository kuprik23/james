{
  "name": "james-ultimate",
  "version": "2.0.0",
  "description": "James Ultimate - AI-Powered Cybersecurity Platform with Multi-LLM Support",
  "main": "src/main.js",
  "bin": {
    "james": "./src/main.js"
  },
  "scripts": {
    "start": "node src/main.js",
    "server": "node src/server.js",
    "dev": "nodemon src/main.js",
    "build": "pkg . --targets node18-win-x64 --output dist/James.exe",
    "build-all": "pkg . --targets node18-win-x64,node18-macos-x64,node18-linux-x64 --out-path dist",
    "test": "jest"
  },
  "pkg": {
    "assets": [
      "src/**/*",
      "public/**/*",
      "config/**/*"
    ],
    "outputPath": "dist"
  },
  "keywords": [
    "cybersecurity",
    "ai",
    "llm",
    "security",
    "iot",
    "ollama",
    "openai",
    "anthropic"
  ],
  "author": "James AI Security Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "boxen": "^5.1.2",
    "cli-table3": "^0.6.3",
    "axios": "^1.6.2",
    "node-fetch": "^2.7.0",
    "systeminformation": "^5.21.20",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "marked": "^11.0.0",
    "highlight.js": "^11.9.0",
    "open": "^8.4.2"
  },
  "devDependencies": {
    "pkg": "^5.8.1",
    "nodemon": "^3.0.2"
  }
}