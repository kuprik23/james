{
  "name": "cybercat-standalone",
  "version": "2.0.0",
  "description": "CYBERCAT - AI-Powered Cybersecurity Analysis Tool with License Management",
  "main": "dist/index.js",
  "bin": {
    "cybercat": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/index.js",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "build-exe": "npm run build && pkg dist/index.js --targets node18-win-x64 --output dist/CyberCat.exe",
    "build-all": "npm run build && pkg dist/index.js --targets node18-win-x64,node18-macos-x64,node18-linux-x64 --out-path dist"
  },
  "pkg": {
    "assets": [
      "templates/**/*",
      "config/**/*"
    ],
    "outputPath": "dist"
  },
  "keywords": [
    "cybersecurity",
    "security",
    "analysis",
    "network",
    "vulnerability"
  ],
  "author": "Emersa Ltd",
  "license": "SEE LICENSE IN TERMS-AND-CONDITIONS.txt",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "boxen": "^5.1.2",
    "cli-table3": "^0.6.3",
    "axios": "^1.6.2",
    "systeminformation": "^5.21.20",
    "node-fetch": "^2.7.0",
    "dns-packet": "^5.6.1",
    "ping": "^0.4.4"
  },
  "devDependencies": {
    "pkg": "^5.8.1",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/inquirer": "^8.2.10"
  }
}
